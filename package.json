{
  "name": "responsive-react",
  "version": "3.0.0",
  "description": "Device-type based rendering of react components (typescript), like render <SideNav> when desktop and <BottomNav> when mobile or tablet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SiddharthaChowdhury/responsive-react.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "prepare": "npm run build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "conditional-render",
    "breakdown",
    "react-typescript",
    "responsive",
    "typescript",
    "react-component",
    "responsive-component",
    "react-responsive",
    "responsive-react",
    "breakpoints",
    "device-based-render",
    "device-type-based-render",
    "screen-size-based-render"
  ],
  "author": "Austin4Silvers",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.48",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.8",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "jest": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "react-dom": "^16.13.1",
    "rollup": "^2.26.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
